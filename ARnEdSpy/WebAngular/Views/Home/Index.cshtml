<!DOCTYPE html>

<html ng-app="WebAngular" ng-controller="LandingPageController">

<head>
    <title ng-bind="models.WebAngular"></title>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.2.0/css/bootstrap.min.css">
    @Styles.Render("~/Content/css")
</head>
<body>


    <div class="navbar navbar-default navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" ng-click="navbarProperties.isCollapsed = !navbarProperties.isCollapsed">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Web Angular Quote Test</a>
            </div>
            <div class="navbar-collapse collapse" collapse="navbarProperties.isCollapsed">
                <ul class="nav navbar-nav">
                        <li><a href="/#/WebAngularUsd/USD">View EUR/USD</a></li>
                        <li><a href="/#/WebAngularUsd/GBP">View EUR/GBP</a></li>
                        <li><a href="/#/WebAngularUsd/AUD">View EUR/AUD</a></li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container mainContent">
        <div ng-view></div>
    </div>
    
  
    <div ng-view></div>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.2.20/angular-route.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.10.0/ui-bootstrap-tpls.min.js"></script>
    @Scripts.Render("~/bundles/WebAngular")
</body>
</html>